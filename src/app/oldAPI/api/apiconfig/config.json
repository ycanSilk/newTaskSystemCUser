{
  "baseUrl": "http://localhost:8083/api",
  "timeout": 5000,
  "headers": {
    "accept": "*/*",
    "Content-Type": "application/json"
  },
  "endpoints": {
    "auth": {
      "register": "/ind/user/register",
      "login": "/ind/user/login",
      "logout": "/ind/user/logout"
    },
    "user": {
      "refreshToken": "/ind/user/refresh",
      "getinduserlist": "/ind/user/list",
      "resetpwd": "/ind/user/reset-password",
      "forgotpwd": "/ind/user/forgot-password",
      "changepwd": "/ind/user/change-password",
      "userprofile": "/ind/user/profile",
      "getinbuserlist": "/ind/user/list",
      "getbizuserstats": "/biz/user/main-accounts/list"
    },
    "task": {
      "accepttask": "/tasks/{taskId}/accept",
      "myacceptedtasks": "/tasks/my-accepted",
      "submittask": "/tasks/accepted/{subTaskId}/submit",
      "missionhall": "/tasks/hall",
      "platformStats": "/tasks/platform-stats"
    },
    "wallet": {
      "walletinfo": "/wallet/info",
      "bindAlipay": "/wallet/bind-alipay",
      "setpaymentpwd": "/wallet/security-password",
      "withdraw": "/wallet/withdraw",
      "availablebalance": "/wallet/available-balance",
      "validatepwd": "/wallet/validate-security-password?password=",
      "transactionrecord": "/wallet/transactions",
      "usersrecharge": "/wallet/recharge"
    },
    "bank": {
      "getbankcardslist": "/bank-cards/",
      "addbankcard": "/bank-cards/",
      "setdefaultbankcard": "/bank-cards/{cardId}/default",
      "getdefaultbankcard": "/bank-cards/default"
    },
    "rental": {
      "publishrental": "/rental/lease/publish",
      "rentalmarket": "/rental/lease/market",
      "requestrentalmarket": "/rental/rent/market",
      "publishrequestrental": "/rental/rent/publish",
      "mypublishrentalinfolist": "/rental/lease/my",
      "myrequestrentalinfolist": "/rental/rent/my",
      "creatleaseorder": "/rental/orders/create",
      "myrentalorder": "/rental/orders/my/lessor",
      "myrentedorder": "/rental/orders/my/renter",
      "getleasestats": "/api/rental/stats"
    },
    "inviteagent": {
      "agentstats": "/agent/stats",
      "myagentteam": "/agent/team",
      "myinvitationcode": "/agent/invitation-code"
    },
    "message": {
      "mymessagelist": "/msgNotice/my-msgList",
      "messagedelete": "/msgNotice/${msgId}/del",
      "messagealready": "/msgNotice/${msgId}/read"
    }
  },
  "mockMode": false,
  "retryCount": 3,
  "retryDelay": 1000
}